<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.kurir.LaporanActivity">
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/linearLayout11" android:padding="16dp" android:background="@color/purple_500">
        <TextView
                android:text="Laporan "
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/textNama" android:textColor="@color/white"
                android:textSize="20sp" android:textStyle="bold"/>
    </LinearLayout>
    <androidx.recyclerview.widget.RecyclerView
            android:layout_width="match_parent"
            android:layout_height="0dp" app:layout_constraintTop_toBottomOf="@+id/linearLayout11"
            app:layout_constraintBottom_toBottomOf="parent" android:id="@+id/rvListLaporanKurir"/>

</androidx.constraintlayout.widget.ConstraintLayout>
